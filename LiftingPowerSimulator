


-- wouldnt work while obfuscated so here u go if u wanna steal it

getgenv().autoLift = false

-- init
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("Facts Hub | Lifting Power Simulator", 5013109572)

-- themes
local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),  
TextColor = Color3.fromRGB(255, 255, 255)
}

-- first page
local page = venyx:addPage("Main", 5012544693)
local section1 = page:addSection("AutoFarm")
local section2 = page:addSection("Misc")

section1:addToggle("Auto Lift", nil, function(value)
    getgenv().autoLift = value
    if value then
        doLift()
    end
end)

section2:addButton("Teleport To Glitch World",function()
        teleportWorld('Glitch')
end)

section2:addButton("Teleport To Volcano World",function()
    teleportAtWorld('VolcanoWorld')
end)

section2:addButton("Teleport To King of The Hill",function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-305.841949, 501.421326, 2606.93433)
end)

section2:addSlider("Walkspeed", 0, -0, 500, function(value)
    game:GetService('Players')['LocalPlayer'].Character.Humanoid.WalkSpeed = value
end)

section2:addSlider("Jump Power", 0, -0, 500, function(value)
    game:GetService('Players')['LocalPlayer'].Character.Humanoid.JumpPower = value
end)

-- second page
local theme = venyx:addPage("Theme", 5012544693)
local colors = theme:addSection("Colors")

for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
colors:addColorPicker(theme, color, function(color3)
venyx:setTheme(theme, color3)
end)
end

-- load
venyx:SelectPage(venyx.pages[1], true)

-- Functions/Scripts

function doLift()
    spawn(function()
        while getgenv().autoLift == true do
            local args = {
                [1] = "Lift",
                [2] = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool")}
                game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool").Handler.Script.RemoteEvent:FireServer(unpack(args))
            wait(0.4)
        end
    end)
end




function teleportTo(placeCFrame)
    local p1 = game.Players.LocalPlayer
    if p1.Character then
        p1.Character.HumanoidRootPart.CFrame = placeCFrame
    end
end
function teleportWorld(world)
    if game:GetService("Workspace"):FindFirstChild(world) then
        teleportTo(game:GetService("Workspace").Glitch.Spawn.CFrame)
    end
end

function teleportAt(placeCFrame)
    local p1 = game.Players.LocalPlayer
    if p1.Character then
        p1.Character.HumanoidRootPart.CFrame = placeCFrame
    end
end
function teleportAtWorld(world2)
    if game:GetService("Workspace"):FindFirstChild(world2) then
        teleportAt(game:GetService("Workspace").VolcanoWorld.Spawn2.CFrame)
    end
end
